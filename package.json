{
  "name": "crud-api",
  "version": "1.0.0",
  "description": "Task is to implement simple CRUD API using in-memory database underneath",
  "author": "Volha Papkouskaya",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start:prod": "npx cross-env NODE_ENV=production ts-node --transpile-only src/index.ts",
    "start:dev": "npx cross-env NODE_ENV=development ts-node-dev src/index.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest --silent",
    "build": "tsc"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.2",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "http": "^0.0.1-security",
    "lodash": "^4.17.21"
  }
}
